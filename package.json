{
  "name": "browserify-bundler",
  "version": "0.0.0",
  "description": "Creates a common package from a collection of scripts",
  "bin": {
    "bundler": "lib/cli"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.js",
    "test": "tape ./spec/*.js",
    "example": "./lib/cli ./examples/basic/bundles -dv -o ./dist -t flowcheck -t babelify"
  },
  "keywords": [
    "babel",
    "es2015",
    "browserify",
    "common",
    "bundler",
    "bundle"
  ],
  "author": "Matt Styles",
  "license": "WTFPL",
  "dependencies": {
    "babel-core": "^5.6.7",
    "browserify": "^10.2.4",
    "chalk": "^1.0.0",
    "glob": "^5.0.10"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.18",
    "eslint": "^0.23.0",
    "tape": "^4.0.0",
    "yargs": "^3.13.0"
  }
}
