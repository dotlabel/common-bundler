{
  "name": "common-bundler",
  "version": "0.3.0",
  "description": "Creates a common package from a collection of script bundles",
  "bin": {
    "bundler": "lib/cli"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.js",
    "test": "tape ./spec/*.js",
    "example": "./lib/cli ./examples/basic/bundles -dv -o ./examples/dist -t flowcheck -t babelify",
    "watch:example": "./lib/cli ./examples/basic/bundles -dv -o ./examples/dist -t babelify --watch 'examples/basic/**/*.js'"
  },
  "keywords": [
    "babel",
    "es2015",
    "flowcheck",
    "browserify",
    "common",
    "bundler",
    "bundle"
  ],
  "author": "Matt Styles",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^5.6.7",
    "browserify": "^10.2.4",
    "chalk": "^1.0.0",
    "chokidar": "^1.0.3",
    "concat-stream": "^1.5.0",
    "eventemitter3": "^1.1.1",
    "factor-bundle": "^2.4.1",
    "glob": "^5.0.10",
    "pretty-hrtime": "^1.0.0",
    "uglifyjs": "^2.4.10",
    "yargs": "^3.13.0"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.18",
    "babelify": "^6.1.2",
    "eslint": "^0.23.0",
    "flowcheck": "^0.2.7",
    "tape": "^4.0.0"
  }
}
